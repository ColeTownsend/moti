{
  "name": "with-expo",
  "displayName": "Moti Expo example",
  "expo": {
    "name": "with-expo",
    "slug": "with-expo",
    "description": "Example app for moti",
    "privacy": "public",
    "version": "1.0.0",
    "jsEngine": "hermes",
    "platforms": ["ios", "android", "web"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nandorojo.with-expo"
    },
    "assetBundlePatterns": ["**/*"],
    "experiments": {
      "turboModules": true
    },
    "plugins": ["expo-dev-client"]
  }
}
